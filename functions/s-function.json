{
  "functions": {
    "authorization": {
      "custom": {
        "excludePatterns": [
          "node_modules/aws-sdk/"
        ],
        "envVars": []
      },
      "handler": "modules/serverless-slackbot/functions/handlers.authorize",
      "timeout": 6,
      "memorySize": 1024,
      "eventSourceArn": "",
      "endpoints": [
        {
          "path": "slackbot/authorization",
          "method": "GET",
          "authorizationType": "none",
          "apiKeyRequired": false,
          "requestParameters": {
            "integration.request.querystring.code": "method.request.querystring.code",
            "integration.request.querystring.state": "method.request.querystring.state"
          },
          "requestTemplates": {
            "application/json": "{\"code\":\"$input.params('code')\"}"
          },
          "responses": {
            "400": {
              "statusCode": "400"
            },
            "default": {
              "statusCode": "200",
              "responseParameters": {},
              "responseModels": {},
              "responseTemplates": {
                "application/json": ""
              }
            }
          }
        }
      ]
    },
    "webhooks": {
      "custom": {
        "excludePatterns": [],
        "envVars": []
      },
      "handler": "modules/serverless-slackbot/functions/handlers.receiveWebhook",
      "timeout": 6,
      "memorySize": 1024,
      "eventSourceArn": "",
      "endpoints": [
        {
          "path": "slackbot/webhooks",
          "method": "GET",
          "authorizationType": "none",
          "apiKeyRequired": false,
          "requestParameters": {},
          "requestTemplates": {
            "application/json": "{\"body\":\"$input.json('$')\"}"
          },
          "responses": {
            "400": {
              "statusCode": "400"
            },
            "default": {
              "statusCode": "200",
              "responseParameters": {},
              "responseModels": {},
              "responseTemplates": {
                "application/json": ""
              }
            }
          }
        }
      ]
    },
    "slashcommands": {
      "custom": {
        "excludePatterns": [],
        "envVars": []
      },
      "handler": "modules/serverless-slackbot/functions/handlers.receiveSlashcommand",
      "timeout": 6,
      "memorySize": 1024,
      "eventSourceArn": "",
      "endpoints": [
        {
          "path": "slackbot/slashcommand",
          "method": "POST",
          "authorizationType": "none",
          "apiKeyRequired": false,
          "requestParameters": {},
          "requestTemplates": {
            "x-www-form-urlencoded": "#set($httpPost=$input.path('$').split(\"&\"))\n{\n#foreach($param in $httpPost)\n#set($keyValue=$param.split(\"=\"))\n\"$keyValue[0]\":\"$util.urlDecode($keyValue[1])\"#if( $foreach.hasNext ),#end\n#end\n}"
          },
          "responses": {
            "400": {
              "statusCode": "400"
            },
            "default": {
              "statusCode": "200",
              "responseParameters": {},
              "responseModels": {},
              "responseTemplates": {
                "application/json": ""
              }
            }
          }
        }
      ]
    }
  }
}



