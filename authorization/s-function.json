{
  "functions": {
    "authorization": {
      "custom": {
        "excludePatterns": [
          "node_modules/aws-sdk/"
        ],
        "envVars": []
      },
      "handler": "modules/serverless-slackbot/authorization/handler.getAccessToken",
      "timeout": 6,
      "memorySize": 1024,
      "eventSourceArn": "",
      "endpoints": [
        {
          "path": "slackbot/authorization",
          "method": "GET",
          "authorizationType": "none",
          "apiKeyRequired": false,
          "requestParameters": {
            "integration.request.querystring.code": "method.request.querystring.code",
            "integration.request.querystring.state": "method.request.querystring.state"
          },
          "requestTemplates": {
            "application/json": "{\"code\":\"$input.params('code')\"}"
          },
          "responses": {
            "400": {
              "statusCode": "400"
            },
            "default": {
              "statusCode": "200",
              "responseParameters": {},
              "responseModels": {},
              "responseTemplates": {
                "application/json": ""
              }
            }
          }
        }
      ]
    }
  }
}