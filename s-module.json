{
  "name": "serverless-slackbot",
  "version": "0.0.1",
  "profile": "aws-0",
  "location": "https://github.com/serverless/serverless-slackbot",
  "runtime": "nodejs",
  "author": "",
  "description": "",
  "custom": {},
  "cloudFormation": {
    "Resources": {
      "DynamoDB": {
        "Type" : "AWS::DynamoDB::Table",
        "Properties": {
          "AttributeDefinitions": [
            {
              "AttributeName": "id",
              "AttributeType": "S"
            }
          ],
          "KeySchema" : [
            {
              "AttributeName": "id",
              "KeyType": "HASH"
            }
          ],
          "ProvisionedThroughput" : {
            "ReadCapacityUnits":  { "Ref" : "DynamoRWThroughput" },
            "WriteCapacityUnits": { "Ref" : "DynamoRWThroughput" }
          },
          "TableName": "serverless-slackbot-teams"
        }
      }
    }
  }
}